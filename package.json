{
  "name": "snapbot",
  "description": "Platform Independent Chatbot Architecture",
  "version": "0.0.1",
  "author": {
    "name": "Travis Fischer",
    "email": "travis@makesnaps.com",
    "url": "https://makesnaps.com"
  },
  "bugs": {
    "url": "https://github.com/SnapsApp/snapbot/issues"
  },
  "dependencies": {
    "async": "^1.4.2",
    "debug": "^2.1.0",
    "inherits": "^2.0.1"
  },
  "devDependencies": {
    "browserify": "^10.0.0",
    "dotenv": "^1.2.0",
    "gulp": "^3.9.0",
    "gulp-gh-pages": "^0.5.2",
    "gulp-npm-run": "0.0.1",
    "ink-docstrap": "git://github.com/fisch0920/docstrap",
    "jsdoc": "^3.3.2",
    "standard": "^4.0.1",
    "tape": "^4.0.0",
    "uglify-js": "^2.4.15"
  },
  "keywords": [
    "chatbot",
    "bot",
    "cross-platform",
    "kik",
    "snapchat",
    "telegram",
    "imessage"
  ],
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/SnapsApp/snapbot.git"
  },
  "scripts": {
    "build": "browserify -s Snapchat -e ./ | uglifyjs -m > snapbot.min.js",
    "build-debug": "browserify -s Snapchat -e ./ > snapbot.debug.js",
    "docs": "node ./node_modules/jsdoc/jsdoc.js -c .jsdoc.json -R README.md",
    "size": "npm run build && cat snapbot.min.js | gzip | wc -c",
    "test": "standard && tape test/*.js",
    "test-node": "tape test/*.js"
  }
}
